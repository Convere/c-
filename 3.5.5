int main() // vector对象拷贝给整型数组
{
	vector<int> num{ 4,5,3,2,9 };
	int score[5]{0};
	for (int i=0;i != num.size();++i)
	{
		score[i] = num[i];
		cout << score[i] << " ";
	}
	cout << endl;
}
