#include <iostream>
#include <vector>
using std::vector;
using namespace std;

int main() //求两两相邻的俩个数的和，若是奇数，最后一个数单独输出
{
	int number;
	vector<int> text;
	while (cin >> number)
	{
		text.push_back(number);
	}
	int a;
	if (text.size() % 2 != 0)
	{
		a = text.size() - 2;
		cout << "最后一个数是" << text[text.size() - 1] << endl;
	}
	else
	{
		a = text.size() - 1;
	}
		for (int i = 0; i <= a; i += 2)
		{

			int sum;
			sum = text[i] + text[i + 1];
			cout << text[i] << "+" << text[i + 1]
				<< "=" << sum << endl;
		}
}

int main() //首尾两数相加 若是奇数则单独输出中间那个数
{
	int number;
	vector<int> text;
	while (cin >> number)
	{
		text.push_back(number);
	}
	int a;
	if (text.size() % 2 != 0)
	{
		a =(text.size() - 1) / 2 - 1;
		cout << "最后一个数是" << text[text.size() - 1] << endl;
	}
	else
	{
		a = text.size() / 2 - 1;
	}
	for (int i = 0; i <= a; i++)
	{

		int sum;
		sum = text[i] + text[text.size()-1-i];
		cout << sum << endl;
	}
}
